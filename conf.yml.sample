go_version: 1.10.0
version: 0.0.1

# testnet: true

db:
  user: your-username
  passwd: your-custom-password
  host: 127.0.0.1
  port: 3306
  database: wormhole
  sql_log: true
redis:
  passwd: your-custom-password
  host: 127.0.0.1
  port: 6379
  db_num: 1
  timeout: 5
  max_idle_conns: 100
  max_open_conns: 300
  initial_open_conns: 10
rpc:
  user: your-rpc-username
  passwd: your-rpc-password
  host: your-rpc-host
  port: your-rpc-port
log:
  filename: app.log
  level: debug
  maxage: 168
  rotationtime: 24
